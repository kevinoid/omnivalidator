<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>omnivalidator@kevinlocke.name</em:id>

        <em:name>Omnivalidator</em:name>
        <!-- Should match EXT_VERSION in resource/omnivalidator/global.js -->
        <em:version>0.1.0</em:version>
        <em:type>2</em:type>
        <em:creator>Kevin Locke</em:creator>

        <em:aboutURL>chrome://omnivalidator/content/aboutdialog.xul</em:aboutURL>
        <em:description>Validate content using any number of configurable validation services.</em:description>
        <em:homepageURL>https://github.com/kevinoid/omnivalidator</em:homepageURL>
        <!-- Note:  Can switch to SVG when dropping support for FF < 4 -->
        <em:iconURL>chrome://omnivalidator/skin/icon-48.png</em:iconURL>
        <em:icon64URL>chrome://omnivalidator/skin/icon-64.png</em:icon64URL>
        <em:optionsURL>chrome://omnivalidator/content/prefwindow.xul</em:optionsURL>

        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.5</em:minVersion>
                <em:maxVersion>14.*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
